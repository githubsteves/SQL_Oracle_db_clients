----- PROCEDURE INSERT_FRAUD -----

CREATE OR REPLACE PROCEDORE SP_INSERT_FRAUD
(  P_FRAUD_STATUS     VARCHAR2,
   P_FRAUD_REASON     VARCHAR2
   P_FRAUD_COMMENT    VARCHAR2 )  AS
   --
   V_FRAUDID  CUSTOMERS.FRAUDID%TYPE;

BEGIN 
   SELECT FRAUDID
   INTO   V_FRAUDID
   FROM   CUSTOMERS;
   -- 
   INSERT INTO FRAUDS ( FRAUDID, FRAUD_STATUS, FRAUD_REASON, FRAUD_COMMENT)
   VALUES  (V_FRAUDID + 10, P_FRAUD_STATUS, P_FRAUS_REASON, P_FRAUD_COMMENT)

END SP_INSERT_FRAUD;
/
