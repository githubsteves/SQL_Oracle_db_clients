----- VIEW26 VW26_ALL_DATA   SELECTION FROM CUSTOMERS, RISK, 
-----                        DOCUMENTS, DEOSITS, WITHDRAWALS, COMPLIANCE, 
-----                        BONUSES, FRAUDS 

select c.last_name,
       r.blacklisted,
       d.poi,
       dep.deposit_amount,
       wd. withdrawal_amount, 
       com.status,
       b.bonusid,
       f.fraud_status
from customers C 
left join risk R         on c.riskid    = r.riskid
left join documents D    on c.docid     = d.docid
